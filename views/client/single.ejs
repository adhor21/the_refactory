      <script>
        $('#demo4').click(function() {
          swal({
              title: "Are you sure?",
              text: "Your will not be able to recover this client!",
              type: "warning",
              showCancelButton: true,
              confirmButtonColor: "#DD6B55",
              confirmButtonText: "Yes, delete it!",
              cancelButtonText: "No, cancel please!",
              closeOnConfirm: false,
              closeOnCancel: false
            },
            function(isConfirm) {
              if (isConfirm) {
                swal("Deleted!", "Your client has been deleted.", "success");
                setTimeout(
                  function() {
                    $("#form").submit();
                  }, 2000);
              } else {
                swal("Cancelled", "Your client is safe ... for now.)", "error");
              }
            });
        });
      </script>
