<% include partials/header %>

<% include nav/main %>

<!-- Main Wrapper -->
<div id="wrapper">
<div class="normalheader ">
    <div class="hpanel">
        <div class="panel-body">
            <a class="small-header-action" href="">
                <div class="clip-header">
                    <i class="fa fa-arrow-up"></i>
                </div>
            </a>
            <div id="hbreadcrumb" class="pull-right m-t-lg">
                <ol class="hbreadcrumb breadcrumb">
                  <a href="/projects/new"><button class="btn btn-primary2"  data-style="zoom-in">New</button> </a>
                </ol>
            </div>
            <h2 class="font-light m-b-xs">
                Projects
            </h2>
        </div>
    </div>
</div>

<div class="content animate-panel">
<div class="row">
        <% projects.forEach(function (project) { %>
        <div class="projects">
            <div class="col-lg-6">
                <div class="hpanel hgreen">
                    <div class="panel-body">
                        <span class="label label-success pull-right"><%=project.status%></span>
                        <div class="row">
                            <div class="col-sm-8">
                                <h4><a href="/projects/<%=project.id%>"> <%=project.name%></a></h4>

                                <p>
                                  <%=project.description%>
                                </p>

                                <div class="row">
                                    <div class="col-sm-3">
                                        <div class="project-label">id</div>
                                        <small><%=project.id%></small>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="project-label">Created</div>
                                        <small><%=project.created_at%></small>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="project-label">Updated</div>
                                        <small><%=project.updated_at%></small>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="project-label">PROGRESS</div>
                                        <div class="progress m-t-xs full progress-small">
                                            <div style="width: 12%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="12" role="progressbar" class=" progress-bar progress-bar-success">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-4 project-info">
                                <div class="project-action m-t-md">
                                    <div class="btn-group">
                                        <button class="btn btn-xs btn-default" href="/projects/<%=project.id%>"> View</button>
                                        <button class="btn btn-xs btn-default" href="/projects/<%=project.id%>/edit"> Edit</button>
                                    </div>
                                </div>
                                <div class="project-value">
                                    <h2 class="text-success">
                                        $1 206,40
                                    </h2>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel-footer">
                        Additional information about project in footer
                    </div>
                </div>
            </div>
            <% }) %>
        </div>
    </div>
</div>
</div>
<% include partials/footer %>
